# Generated by Chef for node <%= node["fqdn"] %>
# Local modifications will be overwritten.
#
# The php-fpm configuration file.
<% @phpfpm.keys.each do |category| %>
	<%= "[#{category}]" %>
	<% @phpfpm[category].keys.each do |key| %>
		<%= "#{key} = #{@phpfpm[category][key]}" %>
	<% end %>
<% end %>

include=<%= node['php']['fpm_pool_dir'] %>/*.conf